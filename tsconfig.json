{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Node 16",

  "include":[
    "src/*.ts"
  ],
  "compilerOptions": {
    "outDir":"js",
    "lib": ["es2021"],
    "module": "commonjs",
    "target": "es2021",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
